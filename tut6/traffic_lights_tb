    .scope S_0x25db050;
T_4 ;
T_4.0 ;
    %delay 10000, 0;
    %load/v 8, v0x25f1820_0, 1;
    %inv 8, 1;
    %set/v v0x25f1820_0, 8, 1;
    %jmp T_4.0;
    %end;
