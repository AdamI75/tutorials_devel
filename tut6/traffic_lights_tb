    .scope S_0x25db050;
T_6 ;
    %wait E_0x25d69b0;
    %ix/load 0, 1, 0;
    %load/vp0 8, v0x25f18d0_0, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0x25f18d0_0, 0, 8;
    %vpi_call 2 63 "$display", "CLOCK: %d, {r,a,g} = {%d,%d,%d}", v0x25f18d0_0, v0x25f1a30_0, v0x25f16d0_0, v0x25f1980_0;
    %jmp T_6;
    .thread T_6;
