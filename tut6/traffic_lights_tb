    %assign/v0 v0x25c74a0_0, 0, 1;
    %load/v 8, v0x25f1130_0, 32;
    %mov 40, 0, 1;
    %addi 8, 1, 33;
    %ix/load 0, 32, 0;
    %assign/v0 v0x25f1130_0, 0, 8;
    %load/v 8, v0x25f1130_0, 32;
    %cmpi/u 8, 10, 32;
    %jmp/0xz  T_2.12, 4;
    %ix/load 0, 32, 0;
    %assign/v0 v0x25f1130_0, 0, 0;
    %ix/load 0, 2, 0;
    %assign/v0 v0x25f1540_0, 0, 0;
