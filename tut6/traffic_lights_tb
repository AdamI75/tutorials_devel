v0x25f1540_0 .var "state", 1 0;
v0x25f1630_0 .net "toggle", 0 0, v0x25f1ab0_0; 1 drivers
E_0x25d69b0 .event posedge, v0x25f1270_0;
    .scope S_0x25db140;
T_0 ;
    %set/v v0x25f1540_0, 0, 2;
    %end;
