    %assign/v0 v0x25c74a0_0, 0, 0;
    %load/v 8, v0x25f1540_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_2.2, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_2.3, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_2.4, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_2.5, 6;
    %ix/load 0, 2, 0;
    %assign/v0 v0x25f1540_0, 0, 0;
    %jmp T_2.7;
T_2.2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x25f13c0_0, 0, 1;
    %load/v 8, v0x25f1630_0, 1;
    %jmp/0xz  T_2.8, 8;
    %ix/load 0, 2, 0;
    %assign/v0 v0x25f1540_0, 0, 1;
    %jmp T_2.9;
T_2.8 ;
    %ix/load 0, 2, 0;
    %assign/v0 v0x25f1540_0, 0, 0;
T_2.9 ;
    %jmp T_2.7;
T_2.3 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0x25f1320_0, 0, 1;
    %load/v 8, v0x25f1630_0, 1;
    %jmp/0xz  T_2.10, 8;
    %movi 8, 2, 2;
    %ix/load 0, 2, 0;
